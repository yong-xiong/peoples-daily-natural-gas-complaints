CREATE TABLE 'cnarea_2020' (
  'id' mediumint(7) unsigned NOT NULL AUTO_INCREMENT,
  'level' tinyint(1) unsigned NOT NULL COMMENT '层级',
  'parent_code' bigint(14) unsigned NOT NULL DEFAULT '0' COMMENT '父级行政代码',
  'area_code' bigint(14) unsigned NOT NULL DEFAULT '0' COMMENT '行政代码',
  'zip_code' mediumint(6) unsigned zerofill NOT NULL DEFAULT '000000' COMMENT '邮政编码',
  'city_code' char(6) NOT NULL DEFAULT '' COMMENT '区号',
  'name' varchar(50) NOT NULL DEFAULT '' COMMENT '名称',
  'short_name' varchar(50) NOT NULL DEFAULT '' COMMENT '简称',
  'merger_name' varchar(50) NOT NULL DEFAULT '' COMMENT '组合名',
  'pinyin' varchar(30) NOT NULL DEFAULT '' COMMENT '拼音',
  'lng' decimal(10,6) NOT NULL DEFAULT '0.000000' COMMENT '经度',
  'lat' decimal(10,6) NOT NULL DEFAULT '0.000000' COMMENT '纬度',
  PRIMARY KEY ('id'),
  UNIQUE KEY 'uk_code' ('area_code') USING BTREE,
  KEY 'idx_parent_code' ('parent_code') USING BTREE
) ENGINE=MyISAM AUTO_INCREMENT=758050 DEFAULT CHARSET=utf8 COMMENT='中国行政地区表';



CREATE TABLE cnarea_2020 (
  id serial PRIMARY KEY,
  level smallint NOT NULL COMMENT '层级',
  parent_code bigint NOT NULL DEFAULT 0 COMMENT '父级行政代码',
  area_code bigint NOT NULL DEFAULT 0 COMMENT '行政代码',
  zip_code integer NOT NULL DEFAULT '000000'::integer COMMENT '邮政编码',
  city_code character(6) NOT NULL DEFAULT '' COMMENT '区号',
  name character varying(50) NOT NULL DEFAULT '' COMMENT '名称',
  short_name character varying(50) NOT NULL DEFAULT '' COMMENT '简称',
  merger_name character varying(50) NOT NULL DEFAULT '' COMMENT '组合名',
  pinyin character varying(30) NOT NULL DEFAULT '' COMMENT '拼音',
  lng decimal(10,6) NOT NULL DEFAULT 0.000000 COMMENT '经度',
  lat decimal(10,6) NOT NULL DEFAULT 0.000000 COMMENT '纬度',
  UNIQUE (area_code),
  INDEX idx_parent_code (parent_code)
)
WITH (
  OIDS = FALSE
);
COMMENT ON TABLE cnarea_2020 IS '中国行政地区表';


CREATE TABLE cnarea_2020 (
  id serial PRIMARY KEY,
  level smallint NOT NULL,
  parent_code bigint NOT NULL DEFAULT 0,
  area_code bigint NOT NULL DEFAULT 0,
  zip_code integer NOT NULL DEFAULT '000000'::integer,
  city_code character(6) NOT NULL DEFAULT '',
  name character varying(50) NOT NULL DEFAULT '',
  short_name character varying(50) NOT NULL DEFAULT '',
  merger_name character varying(50) NOT NULL DEFAULT '',
  pinyin character varying(30) NOT NULL DEFAULT '',
  lng decimal(10,6) NOT NULL DEFAULT 0.000000,
  lat decimal(10,6) NOT NULL DEFAULT 0.000000,
  UNIQUE (area_code),
  INDEX idx_parent_code (parent_code)
)
WITH (
  OID = FALSE
);


CREATE TABLE cnarea_2020 (
  id serial PRIMARY KEY,
  level smallint NOT NULL,
  parent_code bigint NOT NULL DEFAULT 0,
  area_code bigint NOT NULL DEFAULT 0,
  zip_code integer NOT NULL DEFAULT '000000'::integer,
  city_code character(6) NOT NULL DEFAULT '',
  name character varying(50) NOT NULL DEFAULT '',
  short_name character varying(50) NOT NULL DEFAULT '',
  merger_name character varying(50) NOT NULL DEFAULT '',
  pinyin character varying(30) NOT NULL DEFAULT '',
  lng decimal(10,6) NOT NULL DEFAULT 0.000000,
  lat decimal(10,6) NOT NULL DEFAULT 0.000000);


